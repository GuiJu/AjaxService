<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>getDataServlet</servlet-name>
    <servlet-class>ajax.getDataServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>addDataSevlet</servlet-name>
    <servlet-class>ajax.addDataServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleteDataServlet</servlet-name>
    <servlet-class>ajax.deleteDataServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>editDataServlet</servlet-name>
    <servlet-class>ajax.editDataServlet</servlet-class>
  </servlet>
  
  <!-- servlet-mapping -->
  <servlet-mapping>
    <servlet-name>getDataServlet</servlet-name>
    <url-pattern>/getData</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>addDataSevlet</servlet-name>
    <url-pattern>/addData</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>deleteDataServlet</servlet-name>
    <url-pattern>/deleteData</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>editDataServlet</servlet-name>
    <url-pattern>/editData</url-pattern>
  </servlet-mapping>
</web-app>
